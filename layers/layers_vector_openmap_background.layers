        OUTLINECOLOR 255 255 255
        OUTLINEWIDTH 2 
        POSITION CC
        TYPE TRUETYPE
        FONT 'opensansbd'
        ANGLE FOLLOW
        BUFFER 2
        PARTIALS FALSE
        TEXT ('[distname]')
        MINDISTANCE 150
        MINFEATURESIZE 150
