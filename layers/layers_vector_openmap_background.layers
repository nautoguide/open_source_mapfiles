##FUNCTIONAL SITE##

LAYER
   MAXSCALEDENOM 10000
   MINSCALEDENOM 1000
   TYPE POLYGON
   STATUS ON
   GROUP "openmap_background"
   DEBUG 5
   NAME "openmap_surface_functionalsite"
   PROJECTION
       "init=epsg:27700"
   END
   CONNECTION "host=localhost dbname=[YOUR DATABASE] user=[YOUR USER] password=[YOUR PASSWORD] port=5432"
    CONNECTIONTYPE POSTGIS
   DATA "wkb_geometry from (SELECT wkb_geometry,ogc_fid,CASE WHEN DISTNAME LIKE '%School%' THEN 'School' WHEN DISTNAME LIKE '%College%' THEN 'College' WHEN DISTNAME LIKE '%University Hospital%' THEN 'Hospital' WHEN DISTNAME LIKE '%University%' THEN 'University' WHEN DISTNAME LIKE '%Hospital%' THEN 'Hospital' WHEN CLASSIFICA = 'Bus Station' THEN 'Bus Station' WHEN CLASSIFICA = 'Coach Station' THEN 'Coach Station' WHEN CLASSIFICA LIKE '%Railway%' THEN '' ELSE  DISTNAME END AS label,sitetheme,distname FROM mapping_os_opendata_openmap.functionalsite)  AS FOO USING UNIQUE ogc_fid using srid=27700"
   SIZEUNITS METERS
   CLASS
       EXPRESSION ("[sitetheme]" = "Air Transport")
       STYLE
           COLOR "#FFFF7F"
           OUTLINECOLOR "#848000"
           WIDTH 2
           LINEJOIN bevel
       END
       LABEL
         COLOR 115 115 115
         OUTLINECOLOR 255 255 255
         OUTLINEWIDTH 4
         SIZE 25
         TYPE TRUETYPE
         FONT "arialbd"
         PARTIALS FALSE
         TEXT ("[label]")
       END
   END
   CLASS
       EXPRESSION ("[sitetheme]" = "Education")
       STYLE
           COLOR  "#DB7FFF"
           OUTLINECOLOR "#7900B2" 
           WIDTH 2
           LINEJOIN bevel
       END
       LABEL
         COLOR 115 115 115
         OUTLINECOLOR 255 255 255
         OUTLINEWIDTH 4
         SIZE 25
         TYPE TRUETYPE
         FONT "arialbd"
         PARTIALS FALSE
         TEXT ("[label]")
       END
   END
  CLASS
       EXPRESSION ("[sitetheme]" = "Medical Care")
       STYLE
           COLOR "#7FFF88"
           OUTLINECOLOR "#007F06"
           WIDTH 2
           LINEJOIN bevel
       END
       LABEL
         COLOR 115 115 115
         OUTLINECOLOR 255 255 255
         OUTLINEWIDTH 4
         SIZE 25
         TYPE TRUETYPE
         FONT "arialbd"
         PARTIALS FALSE
         TEXT ("[label]")
       END
   END
  CLASS
       EXPRESSION ("[sitetheme]" = "Road Transport")
       STYLE
           COLOR "#FF8A7F"
           OUTLINECOLOR "#96241E"
           WIDTH 2
           LINEJOIN bevel
       END
       LABEL
         #MAXSCALEDENOM 2500
         COLOR 115 115 115
         OUTLINECOLOR 255 255 255
         OUTLINEWIDTH 4
         SIZE 25
         TYPE TRUETYPE
         FONT "arialbd"
         PARTIALS FALSE
         TEXT ("[label]")
       END
   END
   CLASS
       EXPRESSION ("[sitetheme]" = "Water Transport")
       STYLE
           COLOR "#7FA8FF"
           OUTLINECOLOR "#1A498E"
           WIDTH 2
           LINEJOIN bevel
       END
