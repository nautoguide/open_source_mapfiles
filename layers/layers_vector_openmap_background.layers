# THIS IS NECESSARY TO MASK OUT THE DEFAULT WATER COLOR SET ABOVE #
  LAYER
   TYPE POLYGON
   STATUS ON
   GROUP "openmap_background"
   DEBUG 1
   NAME "vector_district_land_polygon"
   PROJECTION
       "init=epsg:27700"
   END
   CONNECTION "host=localhost dbname=[YOUR DATABASE] user=[YOUR USER] password=[YOUR PASSWORD] port=5432"
