##RAILWAY STATION##
LAYER
    MAXSCALEDENOM 10000
    MINSCALEDENOM 1000
    CONNECTION "host=localhost dbname=[YOUR DATABASE] user=[YOUR USER] password=[YOUR PASSWORD] port=5432"
    CONNECTIONTYPE POSTGIS
    DATA "wkb_geometry from (SELECT wkb_geometry,ogc_fid,classifica,distname FROM mapping_os_opendata_openmap.railwaystation)  AS FOO USING UNIQUE ogc_fid using srid=27700"
    DEBUG 5
    GROUP "openmap_background"
    NAME "openmap_railway_station"
    STATUS ON
    TYPE POINT
    SIZEUNITS METERS
    CLASSITEM 'classifica'

    CLASS
      EXPRESSION /^Light.*/
      ##CASE
      STYLE
        COLOR 253 238 85
        OUTLINECOLOR 80 80 80
        WIDTH 4
        SIZE 30
        SYMBOL "sld_mark_symbol_circle_filled"
      END
    END 
     CLASS
      EXPRESSION /.*/
      ##CASE
      STYLE
        COLOR 235 108 177
        OUTLINECOLOR 150 150 150
        #OUTLINEWIDTH 10
        WIDTH 4
        SIZE 30
        SYMBOL "sld_mark_symbol_circle_filled"
      END
      LABEL
        SIZE 24
        COLOR 115 115 115
        OUTLINECOLOR 255 255 255
        OUTLINEWIDTH 2 
        POSITION lc
        OFFSET 0 2
        TYPE TRUETYPE
        FONT 'ariali'
        ANGLE FOLLOW
        BUFFER 2
        PARTIALS FALSE
        TEXT ('[distname]')
      END
    END 
  END 

